<h1>Wall to Wall</h1>

<p style="font-size:150%;">
  <strong><%= link_to h(@person1.name), person_path(@person1) %></strong> +
  <strong><%= link_to h(@person2.name), person_path(@person2) %></strong>
</p>

<p><em>newest messages appear first</em></p>

<%= render :partial => 'messages', :locals => {:messages => @messages} %>